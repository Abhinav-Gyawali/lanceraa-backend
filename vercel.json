{
  "version": 2,
  "builds": [
    {
      "src": "asgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      },
      {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "."
      }
    }
    },
    
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "asgi.py"
    },
    { "src": "app/main.py", "methods": ["GET", "POST"] }
  ]
}
